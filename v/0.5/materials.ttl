#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/digitalconstruction/0.5/Entities#hasMaterial
dice:hasMaterial rdf:type owl:ObjectProperty ;
                 owl:equivalentProperty dicm:hasMaterial ;
		 rdfs:label "hasMaterial" .


###  https://w3id.org/digitalconstruction/0.5/Materials#InnerLayer
dicm:InnerLayer rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf dicm:hasLayer ;
                owl:inverseOf dicm:IsInnerLayerOf ;
                rdfs:range dicm:Layer ;
		rdfs:label "InnerLayer" .


###  https://w3id.org/digitalconstruction/0.5/Materials#IsInnerLayerOf
dicm:IsInnerLayerOf rdf:type owl:ObjectProperty ;
                    rdfs:domain dicm:Layer ;
		    rdfs:label "IsInnerLayerOf" .


###  https://w3id.org/digitalconstruction/0.5/Materials#IsOuterLayerOf
dicm:IsOuterLayerOf rdf:type owl:ObjectProperty ;
                    owl:inverseOf dicm:OuterLayer ;
                    rdfs:domain dicm:Layer ;
		    rdfs:label "IsOuterLayerOf" .


###  https://w3id.org/digitalconstruction/0.5/Materials#OuterLayer
dicm:OuterLayer rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf dicm:hasLayer ;
                rdfs:range dicm:Layer ;
                owl:propertyChainAxiom ( [ owl:inverseOf dicm:adjacentElement
                                         ]
                                         dicm:adjacentElementLayer
                                       ) ;
		rdfs:label "OuterLayer" .


###  https://w3id.org/digitalconstruction/0.5/Materials#adjacentElement
dicm:adjacentElement rdf:type owl:ObjectProperty ;
                     rdfs:range dice:BuildingObject ;
		     rdfs:label "adjacentElement" .


###  https://w3id.org/digitalconstruction/0.5/Materials#adjacentElementLayer
dicm:adjacentElementLayer rdf:type owl:ObjectProperty ;
                          rdfs:range dicm:Layer ;
			  rdfs:label "adjacentElementLayer" .


###  https://w3id.org/digitalconstruction/0.5/Materials#adjacentLayer
dicm:adjacentLayer rdf:type owl:ObjectProperty ,
                            owl:SymmetricProperty ;
                   rdfs:domain dicm:Layer ;
                   rdfs:range dicm:Layer ;
		   rdfs:label "adjacentLayer" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasAcidityConcentration
dicm:hasAcidityConcentration rdf:type owl:ObjectProperty ;
                             rdfs:range dicm:MaterialChemicalProperty ;
                             rdfs:comment "Maximum CaCO3 equivalent that would neutralize the acid;"^^xsd:string ;
			     rdfs:label "hasAcidityConcentration" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasAlkalinityConcentration
dicm:hasAlkalinityConcentration rdf:type owl:ObjectProperty ;
                                rdfs:range dicm:MaterialChemicalProperty ;
                                rdfs:comment "Maximum alkalinity concentration (maximum sum of concentrations of each of the negative ions substances measured as CaCO3)"^^xsd:string ;
				rdfs:label "hasAlkalinityConcentration" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasBoilingPoint
dicm:hasBoilingPoint rdf:type owl:ObjectProperty ;
                     rdfs:domain dicm:Material ;
                     rdfs:range dicm:MaterialThermalProperty ;
		     rdfs:label "hasBoilingPoint" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasCO2Content
dicm:hasCO2Content rdf:type owl:ObjectProperty ;
                   rdfs:domain dicm:Material ;
                   rdfs:range dicm:MaterialCombustionProperty ;
		   rdfs:label "hasCO2Content" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasCOContent
dicm:hasCOContent rdf:type owl:ObjectProperty ;
                  rdfs:domain dicm:Material ;
                  rdfs:range dicm:MaterialCombustionProperty ;
		  rdfs:label "hasCOContent" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasCarbonContent
dicm:hasCarbonContent rdf:type owl:ObjectProperty ;
                      rdfs:domain dicm:Material ;
                      rdfs:range dicm:MaterialFuelProperty ;
                      rdfs:comment "The carbon content in the fuel; This is measured in weight of carbon per unit weight of fuel and is therefore unitless [ISO 16739-1:2018]"^^xsd:string ;
		      rdfs:label "hasCarbonContent" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasCombustionTemperature
dicm:hasCombustionTemperature rdf:type owl:ObjectProperty ;
                              rdfs:domain dicm:Material ;
                              rdfs:range dicm:MaterialFuelProperty ;
                              rdfs:comment "Combustion temperature of the material when air is at 298 K and 100 kPa [ISO 16739-1:2018]"^^xsd:string ;
			      rdfs:label "hasCombustionTemperature" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasConstituent
dicm:hasConstituent rdf:type owl:ObjectProperty ;
                    rdfs:range dicm:Constituent ;
                    owl:propertyChainAxiom ( dicm:hasConstituentSet
                                             dicm:hasConstituent
                                           ) ;
		    rdfs:label "hasConstituent" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasConstituentSet
dicm:hasConstituentSet rdf:type owl:ObjectProperty ;
                       rdfs:range dicm:ConstituentSet ;
		       rdfs:label "hasConstituentSet" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasDynamicViscosity
dicm:hasDynamicViscosity rdf:type owl:ObjectProperty ;
                         rdfs:domain dicm:Material ;
                         rdfs:range dicm:MaterialMechanicalProperty ;
                         rdfs:comment "A measure of the viscous resistance of the material [ISO 16739-1:2018]"^^xsd:string ;
			 rdfs:label "hasDynamicViscosity" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasFreezingPoint
dicm:hasFreezingPoint rdf:type owl:ObjectProperty ;
                      rdfs:domain dicm:Material ;
                      rdfs:range dicm:MaterialThermalProperty ;
		      rdfs:label "hasFreezingPoint" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasGasPressure
dicm:hasGasPressure rdf:type owl:ObjectProperty ;
                    rdfs:domain dicm:Material ;
                    rdfs:range dicm:MaterialEnergyProperty ;
                    rdfs:comment "Fill pressure (for insance, for between-pane gas fills): the pressure exerted by a mass of gas confined in a constant volume [ISO 16739-1:2018]"^^xsd:string ;
		    rdfs:label "hasGasPressure" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasHardness
dicm:hasHardness rdf:type owl:ObjectProperty ;
                 rdfs:domain dicm:Material ;
                 rdfs:range dicm:MaterialChemicalProperty ;
                 rdfs:comment "Water hardness as positive, multivalent ion concentration in the water (usually concentrations of calcium and magnesium ions in terms of calcium carbonate)"^^xsd:string ;
		 rdfs:label "hasHardness" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasHigherHeatingValue
dicm:hasHigherHeatingValue rdf:type owl:ObjectProperty ;
                           rdfs:domain dicm:Material ;
                           rdfs:range dicm:MaterialFuelProperty ;
                           rdfs:comment "Higher Heating Value is defined as the amount of energy released (MJ/kg) when a fuel is burned completely, and H2O is in liquid form in the combustion products [ISO 16739-1:2018]"^^xsd:string ;
			   rdfs:label "hasHigherHeatingValue" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasIsothermalMoistureCapacity
dicm:hasIsothermalMoistureCapacity rdf:type owl:ObjectProperty ;
                                   rdfs:domain dicm:Material ;
                                   rdfs:range dicm:MaterialHygroscopicProperty ;
                                   rdfs:comment "Based on water vapor density [ISO 16739-1:2018]"^^xsd:string ;
				   rdfs:label "hasIsothermalMoistureCapacity" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasLayer
dicm:hasLayer rdf:type owl:ObjectProperty ;
              rdfs:range dicm:Layer ;
              owl:propertyChainAxiom ( dicm:hasLayerSet
                                       dicm:InnerLayer
                                     ) ,
                                     ( dicm:hasLayerSet
                                       dicm:OuterLayer
                                     ) ,
                                     ( dicm:hasLayerSet
                                       dicm:hasLayer
                                     ) ;
 	     rdfs:label "hasLayer" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasLayerSet
dicm:hasLayerSet rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:range dicm:LayerSet ;
		 rdfs:label "hasLayerSet" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasLowerHeatingValue
dicm:hasLowerHeatingValue rdf:type owl:ObjectProperty ;
                          rdfs:domain dicm:Material ;
                          rdfs:range dicm:MaterialFuelProperty ;
                          rdfs:comment "Lower Heating Value is defined as the amount of energy released (MJ/kg) when a fuel is burned completely, and H2O is in vapor form in the combustion products [ISO 16739-1:2018]"^^xsd:string ;
			  rdfs:label "hasLowerHeatingValue" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasLowerVaporResistanceFactor
dicm:hasLowerVaporResistanceFactor rdf:type owl:ObjectProperty ;
                                   rdfs:domain dicm:Material ;
                                   rdfs:range dicm:MaterialHygroscopicProperty ;
                                   rdfs:comment "The vapor permeability relationship of air/material (typically value > 1), measured in low relative humidity (typically in 0/50 % RH) [ISO 16739-1:2018]"^^xsd:string ;
				   rdfs:label "hasLowerVaporResistanceFactor" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasMassDensity
dicm:hasMassDensity rdf:type owl:ObjectProperty ;
                    rdfs:domain dicm:Material ;
                    rdfs:range dicm:MaterialPhysicalProperty ;
                    rdfs:comment "Material mass density [ISO 16739-1:2018]"^^xsd:string ;
		    rdfs:label "hasMassDensity" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasMaterial
dicm:hasMaterial rdf:type owl:ObjectProperty ;
                 rdfs:range dicm:Material ;
                 owl:propertyChainAxiom ( dicm:hasConstituent
                                          dicm:hasMaterial
                                        ) ,
                                        ( dicm:hasLayer
                                          dicm:hasMaterial
                                        ) ,
                                        ( dicm:hasProfile
                                          dicm:hasMaterial
                                        ) ;
		rdfs:label "hasMaterial" .

###  https://w3id.org/digitalconstruction/Entities#hasMaterial
:hasMaterialBatch rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf dice:hasParticipant ;
             owl:inverseOf dice:isMateriaBatchlIn ;
             rdfs:domain dice:Process ;
             rdfs:range :MaterialBatch ;
             rdfs:label "hasMaterialBatch" .

:isMaterialBatchIn rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf dice:isParticipantIn ;
	      owl:inverseOf dice:hasMaterialBatch ;
              rdfs:domain :MaterialBatch ;
              rdfs:range dice:Process ;
              rdfs:label "isMaterialBatchIn" .


