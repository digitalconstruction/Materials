###  https://w3id.org/digitalconstruction/0.5/Materials#hasMoistureCapacityThermalGradient
dicm:hasMoistureCapacityThermalGradient rdf:type owl:ObjectProperty ;
                                        rdfs:domain dicm:Material ;
                                        rdfs:range dicm:MaterialEnergyProperty ;
                                        rdfs:comment "Thermal gradient coefficient for moisture capacity. Based on water vapor density.[ISO 16739-1:2018]"^^xsd:string ;
					rdfs:label "hasMoistureCapacityThermalGradient" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasMoistureDiffusivity
dicm:hasMoistureDiffusivity rdf:type owl:ObjectProperty ;
                            rdfs:domain dicm:Material ;
                            rdfs:range dicm:MaterialHygroscopicProperty ;
                            rdfs:comment """Moisture diffusivity is a transport property that is frequently used in the hygrothermal analysis of building envelope components.
Moisture diffusivity is a transport property that is frequently used in the hygrothermal analysis of building envelope components. [ISO 16739-1:2018]"""^^xsd:string ;
  	 	     	    rdfs:label "hasMoistureDiffusivity" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasMolecularWeight
dicm:hasMolecularWeight rdf:type owl:ObjectProperty ;
                        rdfs:domain dicm:Material ;
                        rdfs:range dicm:MaterialPhysicalProperty ;
			rdfs:label "hasMolecularWeight" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasN20Content
dicm:hasN20Content rdf:type owl:ObjectProperty ;
                   rdfs:domain dicm:Material ;
                   rdfs:range dicm:MaterialCombustionProperty ;
		   rdfs:label "hasN20Content" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasPHLevel
dicm:hasPHLevel rdf:type owl:ObjectProperty ;
                rdfs:domain dicm:Material ;
                rdfs:range dicm:MaterialChemicalProperty ;
                rdfs:comment "Maximum water PH in a range from 0-14."^^xsd:string ;
		rdfs:label "hasPHLevel" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasPoissonRatio
dicm:hasPoissonRatio rdf:type owl:ObjectProperty ;
                     rdfs:domain dicm:Material ;
                     rdfs:range dicm:MaterialMechanicalProperty ;
                     rdfs:comment "A measure of the lateral deformations in the elastic range. [ISO 16739-1:2018]"^^xsd:string ;
		     rdfs:label "hasPoissonRatio" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasPorosity
dicm:hasPorosity rdf:type owl:ObjectProperty ;
                 rdfs:domain dicm:Material ;
                 rdfs:range dicm:MaterialPhysicalProperty ;
		 rdfs:label "hasPorosity" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasProfile
dicm:hasProfile rdf:type owl:ObjectProperty ;
                rdfs:range dicm:Profile ;
                owl:propertyChainAxiom ( dicm:hasProfileSet
                                         dicm:hasProfile
                                       ) ;
	       rdfs:label "hasProfile" .


###  https://w3id.org/digitalconstruction/0.5/Materials#hasProfileSet
dicm:hasProfileSet rdf:type owl:ObjectProperty ;
                   rdfs:range dicm:ProfileSet ;
		   rdfs:label "hasProfileSet" .


